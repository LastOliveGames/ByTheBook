{
  "name": "playwright",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "rm -rf firebase-export-* && firebase emulators:exec \"yarn vite-dev\" --project=demo-dev --import=./emulator_data --export-on-exit --ui --only=auth,functions,database,storage",
    "vite-dev": "vite client",
    "build": "vite build client",
    "preview": "vite preview client",
    "install-all": "yarn install && yarn --cwd client install && yarn --cwd functions install",
    "update": "yarn upgrade-interactive --latest && yarn --cwd client upgrade-interactive --latest && yarn --cwd functions upgrade-interactive --latest"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-vue2": "^2.0.1",
    "eslint": "^8.27.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.7.0",
    "firebase-tools": "^11.16.0",
    "fireplan": "^3.1.0",
    "magic-string": "^0.26.7",
    "sass": "~1.32",
    "typescript": "^4.8.4",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.3",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-inspect": "^0.7.7",
    "vite-tsconfig-paths": "^3.5.2",
    "vue-template-compiler": "^2.7.13",
    "vue-tsc": "^1.0.9"
  }
}
